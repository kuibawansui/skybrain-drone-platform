# SkyBrain 智能无人机管理平台环境变量配置

# 应用配置
NEXT_PUBLIC_APP_NAME=SkyBrain智能无人机管理平台
NEXT_PUBLIC_APP_VERSION=1.0.0
NEXT_PUBLIC_APP_DESCRIPTION=基于AI的智能无人机管理和风险评估平台

# WebSocket配置
NEXT_PUBLIC_WS_URL=wss://your-websocket-server.com/ws
NEXT_PUBLIC_WS_RECONNECT_ATTEMPTS=5
NEXT_PUBLIC_WS_RECONNECT_INTERVAL=3000

# API配置
NEXT_PUBLIC_API_BASE_URL=https://your-api-server.com/api
NEXT_PUBLIC_API_TIMEOUT=10000

# 地图和定位服务
NEXT_PUBLIC_MAP_API_KEY=your_map_api_key_here
NEXT_PUBLIC_WEATHER_API_KEY=your_weather_api_key_here
NEXT_PUBLIC_AIRSPACE_API_KEY=your_airspace_api_key_here

# 3D渲染配置
NEXT_PUBLIC_3D_QUALITY=high
NEXT_PUBLIC_3D_SHADOWS=true
NEXT_PUBLIC_3D_ANTIALIASING=true

# 数据更新频率（毫秒）
NEXT_PUBLIC_RISK_UPDATE_INTERVAL=5000
NEXT_PUBLIC_DRONE_UPDATE_INTERVAL=1000
NEXT_PUBLIC_WEATHER_UPDATE_INTERVAL=30000

# 部署环境
NODE_ENV=production
VERCEL_URL=your-vercel-app-url.vercel.app

# 安全配置
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_ENABLE_ERROR_REPORTING=true